{
  "metadata": {
    "version": "3.0",
    "description": "Router presets aligned with src.config.browser.RouterSettings.",
    "usage": "Copy one of the presets under browser.router in a template or environment override."
  },
  "presets": {
    "balanced": {
      "summary": "Even split between lightweight scraping and browser automation.",
      "router": {
        "hard_domains": [
          "openai.com",
          "anthropic.com",
          "github.com",
          "notion.so"
        ],
        "rate_limits": {
          "lightweight": {
            "max_requests": 12,
            "period_seconds": 1.0
          },
          "crawl4ai": {
            "max_requests": 4,
            "period_seconds": 1.0
          },
          "playwright": {
            "max_requests": 2,
            "period_seconds": 1.0
          },
          "browser_use": {
            "max_requests": 1,
            "period_seconds": 2.0
          },
          "firecrawl": {
            "max_requests": 1,
            "period_seconds": 3.0
          }
        },
        "per_attempt_cap_ms": 20000,
        "min_budget_ms": 750,
        "monitor_failures": true,
        "unavailable_retry_seconds": 60.0
      }
    },
    "interactive_first": {
      "summary": "Bias towards interactive providers for SPA heavy targets.",
      "router": {
        "hard_domains": [
          "vercel.com",
          "netlify.com",
          "linear.app",
          "figma.com"
        ],
        "rate_limits": {
          "lightweight": {
            "max_requests": 6,
            "period_seconds": 1.0
          },
          "crawl4ai": {
            "max_requests": 3,
            "period_seconds": 1.0
          },
          "playwright": {
            "max_requests": 3,
            "period_seconds": 2.0
          },
          "browser_use": {
            "max_requests": 2,
            "period_seconds": 3.0
          },
          "firecrawl": {
            "max_requests": 1,
            "period_seconds": 5.0
          }
        },
        "per_attempt_cap_ms": 25000,
        "min_budget_ms": 1000,
        "monitor_failures": true,
        "unavailable_retry_seconds": 90.0
      }
    }
  }
}
